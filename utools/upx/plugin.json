{
  "pluginName": "Text Processor",
  "description": "使用 flatMap 处理文本内容. 文本处理 字符串处理 文字处理 string映射",
  "version": "2.0.0",
  "author": "paso",
  "main": "web/index.html",
  "preload": "index.js",
  "logo": "logo.png",
  "development": {
    "main": "http://localhost:10987"
  },
  "pluginSetting": {
    "single": true
  },
  "features": [
    {
      "code": "launch",
      "explain": "打开主面板",
      "icon": "",
      "cmds": [
        "文本处理",
        "字符串处理",
        "Text Processor",
        "String Processor"
      ]
    },
    {
      "code": "process",
      "explain": "选择流程处理该文本",
      "icon": "",
      "cmds": [
        {
          "type": "over",
          "label": "选择流程处理该文本",
          "maxLength": 999999999
        }
      ]
    }
  ]
}
